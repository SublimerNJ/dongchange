/* === REPLICATION FIXES === */
html, body { overflow-x: hidden !important; max-width: 100vw !important; }
.mc_2 { overflow: hidden; }
.mc_8 { overflow: hidden; }
#main .mc_4 .img_box video { width: 100%; display: block; aspect-ratio: 16/9; background: #000; }

/* Neutralize skin-news-style.css broad .img_box/.text_box selectors on main page */
#main .img_box { max-width: none !important; flex: unset !important; }
#main .mc_6 .right .img_box { max-width: 820px !important; }
#main .text_box { flex: unset !important; flex-direction: unset !important; gap: unset !important; line-height: inherit !important; display: block !important; }
#main .mc_9 .list .item .text_box { display: flex !important; flex-direction: row !important; }
#main .text_box p { font-size: inherit; font-weight: inherit; margin: inherit; }
#main .text_box span { font-size: inherit; color: inherit; margin: inherit; }
#main .text_box b { font-size: inherit; color: inherit; margin: inherit; }
/* === END REPLICATION FIXES === */

/* common */
@import url('https://fonts.googleapis.com/css2?family=Playfair+Display:ital,wght@0,400..900;1,400..900&display=swap');
.mo {display: none;}
.no_scroll {height:100%;overflow:hidden}
/* header */

#header {position: fixed; top: 0; left: 0; z-index: 1000; width: 100%; transition: all 0.3s;}
#header .black {display: none !important;}
#header > .container {display: flex; align-items: center; justify-content: space-between; height: 110px;}
#header .logo {width: 220px;}
#header .logo img {height: 38px;}
#header .gnb ul {display: flex;}
#header .gnb ul li {display:flex;align-items:center;justify-content:center;padding: 45.5px 0px; width: 144px; text-align: center;}
#header .gnb ul li.active {background: #E9EFF5;}
#header .gnb ul li a {font-size: 17px; color: #fff; font-weight: 700;}
#header .util,
#sidebar .util {display: flex; align-items: center; justify-content: flex-end; width: 220px;}
#sidebar .util {width:auto;margin-right:10px}
#header .util .search_box,
#sidebar .util .search_box {margin-right: 24px;}
#sidebar .util .search_box {margin-right:10px}
#sidebar .util .search_box a {display:block;}
#header .util .search_box img,
#sidebar .util .search_box img {height: 24px;}
#header .util .lang_box,
#sidebar .util .lang_box {position: relative;}
#header .util .lang_box button,
#sidebar .util .lang_box button {display: flex; align-items: center; gap: 6px; padding: 6px 10px; box-sizing: border-box; border: 1px solid #fff; border-radius: 100px;}
#sidebar .util .lang_box button {border-color:var(--main_color)}
#header .util .lang_box button p,
#sidebar .util .lang_box button p {font-size: 17px; color: #fff; font-weight: 600;}
#sidebar .util .lang_box button p {color:var(--main_color)}
#header .util .lang_box .dropdown,
#sidebar .util .lang_box .dropdown {position: absolute; display: none; top: 44px; left: 50%; transform: translateX(-50%); width: fit-content; padding: 20px; box-sizing: border-box; background: #fff; border: 1px solid #D9D9D9; border-radius: 6px;}
#header .util .lang_box .dropdown a,
#sidebar .util .lang_box .dropdown a {font-size: 17px; font-weight: 400; color: #6A6A6A; margin-bottom: 20px; display: block;}
#header .util .lang_box .dropdown a:last-child,
#sidebar .util .lang_box .dropdown a:last-child {margin-bottom: 0;}
#header .util .lang_box .dropdown a:hover,
#sidebar .util .lang_box .dropdown a:hover {color: #111; font-weight: 700;}
#header .util .menu_box {margin-left: 44px;}
#header .util .menu_box button {display: flex; align-items: center;}
#header .util .location_box {display: none;}

#header .gnb_drop {display: none; position: absolute; left: 0; width: 100%; background: #fff; border-bottom: 1px solid #E2E2E2; border-top: 1px solid #E2E2E2;}
#header .gnb_drop .container {display: flex; justify-content: space-between;}
#header .gnb_drop .position {width: 220px;}
#header .gnb_drop .sub_gnb > ul {display: flex;}
#header .gnb_drop .sub_gnb > ul > li {padding: 40px 20px 75px; box-sizing: border-box; width: 144px; text-align: center; border-right: 1px solid #E2E2E2; display: flex; flex-direction: column; align-items: center;}
#header .gnb_drop .sub_gnb > ul > li.active {background: #E9EFF5;}
#header .gnb_drop .sub_gnb > ul > li:first-child {border-left: 1px solid #E2E2E2;}
#header .gnb_drop .sub_gnb > ul > li a {margin-bottom: 20px; font-size: 17px; font-weight: 700; color: #3A3A3A; position: relative; display: inline-block;}
#header .gnb_drop .sub_gnb > ul > li a:last-child {margin-bottom: 0;}
#header .gnb_drop .sub_gnb > ul > li a::before {width: 5px; height: 5px; background: var(--main_color); border-radius: 100%; position: absolute; transition: all 0.3s; left: 0; top: 50%; transform: translateY(-50%); content: ''; opacity: 0;}
#header .gnb_drop .sub_gnb > ul > li a:hover {color: var(--main_color);}
#header .gnb_drop .sub_gnb > ul > li a:hover::before {left: -10px; opacity: 1;}

#header.active {background: #fff;}
#header.active .black {display: block !important;}
#header.active .white {display: none !important;}
#header.active .gnb ul li a {color: #121212;}
#header.active .util .lang_box button {border: 1px solid var(--main_color);}
#header.active .util .lang_box button p {color: var(--main_color);}

#header.fixed {background: #fff; box-shadow: rgba(149, 157, 165, 0.2) 0px 8px 24px;}
#header.fixed .black {display: block !important;}
#header.fixed .white {display: none !important;}
#header.fixed .gnb ul li a {color: #121212;}
#header.fixed .util .lang_box button {border: 1px solid var(--main_color);}
#header.fixed .util .lang_box button p {color: var(--main_color);}

.lang_box button.on {border: 1px solid var(--main_color);}
.lang_box button.on p {color: var(--main_color);}

#sidebar {position: fixed; left: 0; top: 0; z-index: 10000; width: 100%; height: 100%; background: rgba(0,0,0,.4); opacity: 0; transition: all 0.3s; visibility: hidden;}
#sidebar.on {visibility: visible; opacity: 1;}
#sidebar .box {width: 100%; height: 100%; background: #fff; max-width: 375px; width: 100%; height: 100%; box-shadow: rgba(149, 157, 165, 0.2) 0px 8px 24px;margin-left:auto}
#sidebar .top {padding: 10px 16px; box-sizing: border-box; display: flex; align-items: center;}
#sidebar .top .logo {margin-right:auto}
#sidebar .top .logo img {height: 24px;}
#sidebar .side_gnb {padding: 30px 20px; box-sizing: border-box;}
#sidebar .side_gnb ul li {margin-bottom: 20px;}
#sidebar .side_gnb ul li:last-child {margin-bottom: 0;}
#sidebar .side_gnb ul li > a {font-size: 20px; font-weight: 800; display: block; padding: 7px 10px; box-sizing: border-box;}
#sidebar .side_gnb ul li .dropdown {display: none;}
#sidebar .side_gnb ul li .dropdown a {display: block; font-size: 15px; color: #3C3C3C; padding: 7px 10px; box-sizing: border-box;}

.float_btns {display:flex;flex-direction:column;gap:15px;position:fixed;bottom:100px;right:40px;z-index:99}
.float_btns > * {display:flex;align-items:center;justify-content:center;width:60px;height:60px;border-radius:60px;font-size:12px;font-weight:700;color:#fff;background:#1C1C1C;box-shadow:0 5px 5px rgba(0,0,0,.25);transition:all .2s;line-height:1.3}
.float_btns > *.mobile {display:none}
.float_btns > *:hover {transform:translateY(-5px)}
.float_btns > *.btn_call {background:#084784}
.float_btns > *.float_form_open {display:none}
.float_btns > * svg {width:24px;height:24px}

.float_form {display:flex;width:100%;position:fixed;bottom:0;left:0;z-index:1001}
.float_form .info {display:flex;align-items:center;justify-content:center;gap:0 20px;font-size:17px;font-weight:500;background:#084784;color:#fff;padding:0 24px}
.float_form .info * {color:inherit}
.float_form .info strong {font-size:1.64em;font-weight:700}
.float_form .form {flex:1;background:#1C1C1C;padding:20px 34px}
.float_form .form .btn_close {display:none}
.float_form form {display:flex;align-items:center;gap:0 24px;color:#fff}
.float_form form .item {display:flex;align-items:center;gap:0 10px}
.float_form form .item .tit {font-size:18px;font-weight:700;color:#fff}
.float_form form .item input,
.float_form form .item textarea {display:block;width:100%;max-width:250px;height:44px;border-radius:5px;background:#333333;border:solid 1px #252525;font-size:17px;color:#fff;padding:0 16px}
.float_form form .item_03 {flex:1}
.float_form form .item textarea {max-width:initial;resize:none;padding:11px 16px}
.float_form form .chk_box input {display:none}
.float_form form .chk_box input:checked + label {color:#fff}
.float_form form .chk_box input:checked + label::before {background:#fff url(../img/check_01.svg) no-repeat center/115%}
.float_form form .chk_box label {display:flex;align-items:center;gap:0 10px;font-size:15px;font-weight:700;color:rgba(255,255,255,.7);cursor:pointer}
.float_form form .chk_box label::before {content:'';display:block;width:18px;height:18px;background:#FFFFFF;border:solid 1px #747474}
.float_form form .submit {display:flex;align-items:center;min-width:128px;height:44px;background:#084784;color:#ffffff;font-size:18px;font-weight:700;padding:0 30px}

/* footer */

#footer {padding: 80px 0 120px; background: #32425C;}
#footer .logo img {height:65px}
#footer .container {display: flex; justify-content: space-between;}
#footer .content .util {display: flex; gap: 30px; align-items: center; margin-bottom: 10px;}
#footer .content .util ul {display: flex; align-items: center; gap: 20px;}
#footer .content .util ul li {padding-right: 20px; position: relative;}
#footer .content .util ul li a,
#footer .content .util ul li button {font-size: 15px; font-weight: 700; color: #fff; line-height: 1.5;}
#footer .content .util ul li::after {display: block; content: ''; position: absolute; right: 0; top: 50%; transform: translateY(-50%); width: 1px; height: 14px; background: #E2E2E2;}
#footer .content .util ul li:last-child {padding-right: 0;}
#footer .content .util ul li:last-child::after {display: none;}
#footer .content .util .sns {display: flex; align-items: center; gap: 12px;}
#footer .content .util .sns a img {height: 34px;}
#footer .content .info {display: flex; gap: 14px; margin-bottom: 20px;}
#footer .content .info p {font-size: 15px; color: #fff; font-weight: 400;}
#footer .content .info p b {font-weight: 700; color: #fff;}
#footer .content .copyrights {font-size: 15px; color: #fff; font-weight: 400;}

#cs_btn {visibility: hidden; opacity: 0; transition: all 0.3s; position: fixed; right: 100px; bottom: 100px; display: flex; align-items: center; gap: 10px; padding: 10px 14px; box-sizing: border-box; background: var(--main_color); font-size: 17px; font-weight: 600; z-index: 10; border-radius: 100px; box-shadow: 0px 4px 30px 0px rgba(0, 0, 0, 0.15);}
#cs_btn.on {visibility: visible; opacity: 1;}
#cs_btn p {color: #fff;}


/* main */
#main {overflow: hidden;}

#main .mv_swiper {position: relative; overflow: hidden;}
#main .mv_swiper .swiper-slide {width: 100%;}
#main .mv_swiper .swiper-slide .container {height: 100vh; display: flex; align-items: center;}
#main .mv_swiper .swiper-slide .item_box {position: relative; z-index: 1;}
#main .mv_swiper .swiper-slide .item_box .img_box {margin-bottom: 34px;}
#main .mv_swiper .swiper-slide .item_box .img_box img {height: 270px;}
#main .main_visual {position:relative}
#main .main_visual .search_box {text-align:center}
#main .main_visual .search_box input {width: 610px; margin-bottom: 20px; height: 54px; padding: 0px 70px 0px 35px; box-sizing: border-box; background: url(../img/search_icon_b.svg) no-repeat rgba(255,255,255,.5) center right 35px / 25px; border-radius: 100px; color: #000; font-size: 21px;}
#main .main_visual .search_box input:focus {background-color:#ffffff}
#main .main_visual .search_box input::placeholder {color: #ffffff;}
#main .main_visual .search_box input:focus::placeholder {color: #ADADAD;}
#main .main_visual .search_box .keyword {display: flex;justify-content:center; flex-wrap: wrap; max-width: 610px; width: 100%; gap: 10px;margin:0 auto}
#main .main_visual .search_box .keyword a:first-child {background: var(--main_color); width: 38px; height: 38px; border-radius: 100%; display: flex; align-items: center; justify-content: center;}
#main .main_visual .search_box .keyword a:first-child img {height: 18px;}
#main .main_visual .search_box .keyword a:nth-child(n+2) {line-height: 38px; padding: 0px 14px; box-sizing: border-box; background: rgba(255, 255, 255, 0.50); color: var(--main_color); font-size: 15px; font-weight: 600; height: 38px; border-radius: 100px; transition: all 0.3s;}
#main .main_visual .search_box .keyword a:nth-child(n+2):hover {background: var(--main_color); color: #fff;}
#main .main_visual .control {display: flex; align-items: center; gap: 24px;}
#main .mv_swiper .swiper-slide .item_box .control .progress {display: flex; align-items: center; gap: 14px;}
#main .mv_swiper .swiper-slide .item_box .control .progress span {font-size: 17px; font-weight: 700;}
#main .mv_swiper .swiper-slide .item_box .control .progress .num_first span {color: var(--main_color);}
#main .mv_swiper .swiper-slide .item_box .control .progress .num_last span {color: #fff;}
#main .mv_swiper .swiper-slide .item_box .control .progress .mv_progress_bar {width: 200px; height: 5px; border-radius: 100px; background: rgba(255, 255, 255, 0.50); overflow: hidden; position: relative;}
#main .mv_swiper .swiper-slide-active .item_box .control .progress .mv_progress_bar::after {position: absolute; left: 0; top: 0; width: 0%; height: 5px; content: ''; background: var(--main_color); animation: progressBarAnimation 4.2s linear infinite;}
#main .video_wrap {width:100%;height:100vh;background:#000}
#main .video_wrap video {width:100%;height:100%;object-fit:cover;opacity:.7}
#main .cont {width:100%;padding:0 20px;position:absolute;top:50%;left:50%;transform:translate(-50%, -50%);z-index:2}
#main .cont .txt_wrap {height:48px;margin-bottom:40px}
#main .cont .txt_wrap .text {font-size:40px;font-weight:600;text-align:center;color:#ffffff}
#main .cont .txt_wrap .text::after {display:inline-block;margin-left:6px;margin-top:-.15em;vertical-align:middle;content:'';background:#fff;width:2px;height:.9em;animation:blink infinite 1s ease;}
@keyframes blink {
    0%	{opacity:0;}
    100%	{opacity:1;}
}
@keyframes progressBarAnimation {
    from {width: 0%;}
    to {width: 100%;}
}

#main .mv_swiper .swiper-slide .item_box .control .btn_box {display: flex; gap: 10px;}
#main .mv_swiper .swiper-slide .item_box .control .btn_box div {cursor: pointer;}
#main .main_visual .wheel {position: absolute; left: 50%; transform: translateX(-50%); bottom: 0; z-index: 10; text-align: center;}
#main .main_visual .wheel p {font-size: 14px; color: #fff; font-weight: 700; margin-bottom: 10px;}
#main .main_visual .wheel img {margin-bottom: 10px;}
#main .main_visual .wheel .border {width: 1px; height: 60px; margin: 0 auto; background: #fff;}

#main .mv_swiper .swiper-slide .bg_box {position: absolute; left: 0; top: 0; width: 100%; height: 100%;}
#main .mv_swiper .swiper-slide-active .bg_box img {animation: mvbgAnimation 4.2s linear forwards;  width: 100%; height: 100%; object-fit: cover; }

@keyframes mvbgAnimation {
    from {transform: scale(1);}
    to {transform: scale(1.1);}
}

#main .mv_swiper .swiper-slide .mv_progress_bar {position: relative;}
#main section .title {min-width: max-content;}
#main section .title p {font-size: 20px; color: var(--main_color); font-weight: 600; font-family: var(--sub_font); margin-bottom: 16px;}
#main section .title h3 {font-size: 54px; font-weight: 700; color: #121212;}
#main section .title article {font-size: 20px; font-weight: 500; color: #646464; margin-top: 17.5px; line-height: 1.4;}

#main .mc_1 {padding: 175px 0px 0px; min-height: 1200px; position: relative;}
#main .mc_1 .float {position: absolute; left: 0; top: 50%; transform: translateY(-50%); z-index: -1;}
#main .mc_1 .float p {font-size: 120px; font-weight: 700; color: #F1FAFF; font-family: var(--sub_font); line-height: 100%;}
#main .mc_1 .container {display: flex; justify-content: space-between;}
#main .mc_1 .left .title {margin-bottom: 44px;}
#main .mc_1 .left .btn_box {display: flex; align-items: center; gap: 24px;}
#main .mc_1 .left .btn_box .mc_1_arrow {display: flex; align-items: center; gap: 14px; cursor: pointer;}
#main .mc_1 .left .btn_box .mc_1_arrow p {font-size: 24px; font-weight: 500; font-family: var(--sub_font); color: #070707;}
#main .mc_1 .left .btn_box .mc_1_arrow:hover p {color: var(--main_color); text-decoration: underline;}
#main .mc_1 .left .btn_box .mc_1_pagination {display: flex; align-items: center; justify-content: center; width: auto;}
#main .mc_1 .left .btn_box .mc_1_pagination .swiper-pagination-bullet-active {background: var(--main_color);}
#main .mc_1 .right {width: 100%; max-width: 1000px;}
#main .mc_1 .right .swiper {padding: 20px; box-sizing: border-box;}
#main .mc_1 .right .swiper-slide {transition: all 0.3s;}
#main .mc_1 .right .swiper-slide .item {width: 100%; height: 420px; overflow: hidden; position: relative; box-shadow: rgba(149, 157, 165, 0.2) 0px 8px 24px;}
#main .mc_1 .right .swiper-slide .item:first-child {margin-bottom: 34px;}
#main .mc_1 .right .swiper-slide .item .text_box {position: absolute; left: 0; transition: all 0.3s; bottom: -90px; padding: 34px; box-sizing: border-box; background: #fff; box-shadow: 0px 0px 21px 0px rgba(201, 218, 232, 0.50); max-width: 335px; width: 100%;}
#main .mc_1 .right .swiper-slide .item .text_box p {font-size: 16px; color: var(--main_color); margin-bottom: 16px; font-weight: 700;}
#main .mc_1 .right .swiper-slide .item .text_box h3 {white-space: nowrap; font-size: 24px; color: #222; font-weight: 700; text-overflow: ellipsis; overflow: hidden; margin-bottom: 8px;}
#main .mc_1 .right .swiper-slide .item .text_box article {font-size: 17px; color: #646464; font-weight: 500; white-space: nowrap; text-overflow: ellipsis; overflow: hidden; margin-bottom: 24px;}
#main .mc_1 .right .swiper-slide .item .text_box .btn_box {display: flex; justify-content: flex-end;}
#main .mc_1 .right .swiper-slide .item .text_box .btn_box a {display: flex; align-items: center; justify-content: center; width: 44px; height: 44px; border-radius: 100%; border: 1px solid #DADADA; background: #FFF; box-shadow: 0px 0px 21px 0px rgba(201, 218, 232, 0.50);}
#main .mc_1 .right .swiper-slide .item:hover .text_box {bottom: 0; border: 1.5px solid var(--main_color);}
#main .mc_1 .right .swiper-slide .item .img_box {font-size: 0px; width: 100%; height: 100%;}
#main .mc_1 .right .swiper-slide .item .img_box img {width: 100%; height: 100%; object-fit: cover;}
#main .mc_1 .right .swiper-slide-active {padding-top: 90px;}

#main .mc_2 {padding: 100px 0px 160px;}
#main .mc_2 .container {max-width: 1280px;}
#main .mc_2 .title {margin-bottom: 60px; text-align: center;}
#main .mc_2 .swiper {overflow: visible;}
#main .mc_2 .swiper .swiper-slide {position: relative; height: 500px; width: 100%;}
#main .mc_2 .swiper .swiper-slide .text_box {position: absolute; left: 50%; top: 50%; transform: translate(-50%, -50%);padding:0 10px}
#main .mc_2 .swiper .swiper-slide .text_box .top {text-align: center; margin-bottom: 34px;}
#main .mc_2 .swiper .swiper-slide .text_box .top h3 {font-size: 40px; font-weight: 700; margin-bottom: 10px; color: #fff;}
#main .mc_2 .swiper .swiper-slide .text_box .bottom ul {max-width: 475px; width: 100%; display: flex; justify-content: center; column-gap: 20px; row-gap: 8px; flex-wrap: wrap;margin:0 auto}
#main .mc_2 .swiper .swiper-slide .text_box .bottom ul li {width: calc(33.3% - 14px); font-size: 18px; font-weight: 500; color: #fff; text-align: center;}
.en #main .mc_2 .swiper .swiper-slide .text_box .bottom ul li {width:calc(50% - 10px)}
#main .mc_2 .swiper .swiper-slide .text_box .bottom ul li b {font-weight: 800; color: #fff; margin-right: 6px;}
#main .mc_2 .swiper .swiper-slide .img_box {width: 100%; height: 100%;}
#main .mc_2 .swiper .swiper-slide .img_box img {width: 100%; height: 100%; object-fit: cover;}
#main .mc_2 .swiper .mc_2_pagination {position: unset; display: flex; justify-content: center; gap: 80px; margin-top: 24px;}
#main .mc_2 .swiper .mc_2_pagination .swiper-pagination-bullet {width: auto; height: auto; background: none; border-radius: 0; opacity: 1;}
#main .mc_2 .swiper .mc_2_pagination .swiper-pagination-bullet span {position: relative; font-size: 24px; font-weight: 700; color: #989898;}
#main .mc_2 .swiper .mc_2_pagination .swiper-pagination-bullet.on span {color: #000;}
#main .mc_2 .swiper .mc_2_pagination .swiper-pagination-bullet.on span::before {position: absolute; content: ''; width: 7px; height: 7px; border-radius: 100%; background: #000; display: block solid #000; left: -13px; top: 5px;}

#main .mc_3 {padding: 150px 0px 0px; background: url(../img/mc_3_bg.jpg) no-repeat center / cover; overflow: hidden;}
#main .mc_3 .container {display:flex;justify-content:space-between;}
#main .mc_3 .left .title {margin-bottom: 40px;}
#main .mc_3 .left .title p {color: #fff;}
#main .mc_3 .left .title h3 {color: #fff;}
#main .mc_3 .left .btn_box {display: flex; align-items: center; gap: 20px;}
#main .mc_3 .left .btn_box .mc_3_arrow {cursor: pointer;}
#main .mc_3 .right {max-width: 987px;}
#main .mc_3 .right .swiper-slide {display: flex; justify-content: flex-end; position: relative;}
#main .mc_3 .right .swiper-slide .img_box {padding-right: 110px; max-width: 560px; box-sizing: border-box;}
#main .mc_3 .right .swiper-slide .img_box img {width: 100%;}
#main .mc_3 .right .swiper-slide .text_box {position: absolute; top: 50%; transform: translateY(-50%); left: 0; width: 100%;}
#main .mc_3 .right .swiper-slide .text_box .info h3 {font-size: 34px; color: #fff; font-weight: 300; line-height: 1.4; margin-bottom: 44px;}
#main .mc_3 .right .swiper-slide .text_box .info h3 b {font-weight: 700; color: #fff;}
